        "--talk-name=org.kde.kpasswdserver",
        "--system-talk-name=org.freedesktop.NetworkManager",
        "--env=QT_QPA_PLATFORM=xcb",
        "--env=SASL_PATH=/app/lib/sasl2"
    ],
    "cleanup": [
        "/lib/cmake",
        "/lib/pkgconfig",
        "/lib/plugins/designer",
        "/include",
        "/docs",
        "/mkspecs",
        "/share/man",
        "/bin/afmtodit",
        "/usr/bin/xsd",
        "/lib/libKF5*.so",
        "/lib/libboost_*.so",
        "*.a",
        "*.la"
